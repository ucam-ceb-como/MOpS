<?xml version="1.0" encoding="ISO-8859-1"?>
<mechanism name="Titanium Dioxide" units="CGS">
           
    <!-- Define particle components -->
    <component id="Rutile">
        <description>Rutile</description>
        <density>4.26</density>
        <molwt>79.87</molwt>
    </component>
	
    <!-- Define particle types (Detailed) -->
    <particle id="[Ru]" model="bintree" subtree="false">
        <description>A rutile particle.</description>
	    <fractdim>1.8</fractdim>
        <coalthresh>1.0</coalthresh>
        <sintering enable="true" model="GBD">
		    <A units="s/m4K">7.44E+16</A>
            <E units="K">31032</E>
		    <Dpmin units="m">0</Dpmin>
        </sintering>
    </particle>
        <!--sintering enable="true" model="Rutile">
		    <A units="s/m4K">3.7E+16</A>
            <E units="K">31032</E>
		    <Dpmin units="nm">3.4</Dpmin>
        </sintering-->
	
	<!-- Define particle types (Spherical) -->
    <!--particle id="[Ru]" model="spherical" subtree="false">
        <description>A rutile particle.</description>
    </particle-->
	
    <!-- Coagulation -->
    <coagulation>
	    <A>1.0</A>
	    <kernel>weightedtransition</kernel>
	    <weightrule>w3</weightrule>
	    <inceptionweightchange flag="on">
	    <maxinceptionweight>10000.0</maxinceptionweight>
	    <mininceptionweight>1.0</mininceptionweight>
	    <onsetsp>1.0</onsetsp>
	    <weightchangefun>exponential</weightchangefun>
	    </inceptionweightchange>
    </coagulation>

    <!-- Ti addition reactions
         A is in cm/s * Avogadro's number
         E is in kcal/mol -->    
    <reaction type="titania" name="TiCl4 addition" form="firstorder" defer="true">
        <formula>[Rutile_n] + TICL4 + 1.0 O2 to Rutile_n+1 + 2.0 CL2</formula>
        <reactant id="TICL4" stoich="+1.0" />
        <reactant id="O2"  stoich="+1.0" />
        <product id="CL2" stoich="+2.0" />
        <particlein  id="[Ru]"/>
        <particleout id="[Ru]"/>
        <!--A>1.2044e+028</A-->
		<!--A>0.0</A-->
        <!--E units="cal">11.9503</E--> 
        <n>0.0</n>
        <A>8.06948e+028</A>
        <E units="cal">14.3404</E> 
        <particleterm id="s" power="1.0"/>
        <component id="Rutile" dx="+1.0"/>
    </reaction>  

